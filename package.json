{
  "name": "my-university-registration",
  "version": "0.0.1",
  "description": "This README would normally document whatever steps are necessary to get your application up and running.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "nodemon --config .nodemon --signal SIGTERM --exec ts-node -r ./src/interface/rest/index.ts --transpile-only",
    "test": "jest",
    "db:task": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile ./src/common/knexfile.ts",
    "db:migrate:up": "npm run db:task migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/alven0920/my-university-registration.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/alven0920/my-university-registration#readme",
  "dependencies": {
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "inversify": "^5.0.5",
    "knex": "^0.21.16",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "os": "^0.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint . --fix"
  }
}
